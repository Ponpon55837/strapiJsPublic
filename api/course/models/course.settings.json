{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "name": "course"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "size": {
      "type": "integer",
      "required": true,
      "unique": true
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "url": {
      "type": "text"
    },
    "published": {
      "type": "datetime"
    }
  }
}
